QAction
{
	font-size:15px; 
	font-family: Arial,"Microsoft YaHei";
}
QToolBar
{
	font-size:15px; 
	font-family: Arial,"Microsoft YaHei";
	background:rgb(207,241,241);
}

QPushButton {
font-size:12px;
font-family: Arial,"Microsoft YaHei";
border-width: 1px 1px 1px 1px;
color: black;
border-style: solid;
border-radius:4px;
border-color: rgb(235,235,235);
border:1px groove gray;border-style:outset;
background: rgb(207,241,241);
}

QPushButton:hover {
background-color:rgb(120, 213, 255);
color:white;
}

QPushButton:pressed {
color: black;
background:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
radius: 1.5, stop: 0 #fff,stop: 1 #fff);
border:1px solid #ccc;
}
QPushButton:selected {
background:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
radius: 1.5, stop: 0 #7ca0d6,stop: 1 #3d72c2);
border:1px solid #325fa3;
}
QPushButton:disabled {
color: #999;
border: 1px solid #bbb;
border-radius: 4px;
font-family:Arial,"Microsoft YaHei";
font-size:14px;
padding: 5px;
background:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
radius: 1.35,stop: 0 #eee,stop: 1 #E3E3E3);
min-width: 50%;
}

QLabel{
font-size:15px;
font-family: Arial,Microsoft YaHei;
border-width: 0px 0px 0px 0px;
border-style: solid;
border-radius:4px;
border-color: rgb(207,241,241);
}

QLineEdit
{
font-size:15px;
font-family: Arial,Microsoft YaHei;
background: rgb(207,241,241);
border-color: rgb(207,241,241);
}
QComboBox
{
font-size:15px;
font-family: Arial,"Microsoft YaHei";
border-width: 0px 0px 0px 0px;
border-style: solid;
border-radius:4px;
background-color:rgb(207,241,241);
border:none;
}

QComboBox:editable,QComboBox::drop-down:editable {
border-width: 0px 0px 0px 0px;
border-style: solid;
border-radius:4px;
background:rgb(207,241,241);
}

QComboBox::drop-down:editable {
border-width: 0px 0px 0px 0px;
border-style: solid;
border-radius:4px;
background:rgb(207,241,241);
}
QComboBox:!editable{
border-style: solid;
border-radius:4px;
background:rgb(207,241,241);
border-width: 0px 0px 0px 0px;
}

QComboBox:!editable:on, QComboBox::drop-down:editable:on {
border-style: solid;
border-radius:4px;
background:rgb(207,241,241);
border-width: 0px 0px 0px 0px;
}

QComboBox:on {
padding-top: 3px;
padding-left: 4px;
}


QComboBox::drop-down {
subcontrol-origin: padding;
subcontrol-position: top right;
width: 40px;
border-left-width: 0px;
border-left-color: gray;
border-left-style: solid;
border-top-right-radius: 3px;
border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
image: url(:/resource/images/arrow.png);
}

QComboBox::down-arrow:on {
top: 1px;
left: 1px;
}

QComboBox QAbstractItemView {
font-size:15px;
font-family: Arial,"Microsoft YaHei";
outline: none;
border:none;
}
QComboBox QAbstractItemView::item {
height: 52px;
}
QComboBox QAbstractItemView::item:selected {
background-color:rgb(129, 182, 226);
border:none;
}

QGroupBox{
padding:4px;
width:100%;
min-height:18px;
font-size:14px;
font-family:Arial,"Microsoft YaHei";
}

QGroupBox{
background-color:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
stop: 0 #CFFAFA,
stop: 1 #CFFFFF);
}

QTableView {
border:none;
selection-background-color: rgb(120, 213, 255);
selection-color:#222;
background-color:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #CFFAFA, stop: 1 #CFFFFF);
color:black;
font-size:14px;
border-radius:2px;
alternate-background-color:rgb(238,243,246);
gridline-color:rgb(24,189,187);
border-style: solid;
border-radius:4px;
}
QTableView QTableCornerButton::section {
border: none;
background-color:#6D6D6D;
}
QHeaderView::section,QHeaderView::section:checked {
color:#333;
font:15px;
/*font-weight:bold;*/
font-family:Arial,"Microsoft YaHei";
min-height:30px;
min-width:30px;
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
stop: 0 #18bdbb,
stop: 0.8 #18bdbb,
stop: 1 #00b4b2);
border:0px solid #BDBDBD;
border-right:1px solid #BDBDBD;
padding-top:3px;
padding-bottom:3px;
border:none;
}
QTableView QHeaderView {
font:15px;
color:#e2ecf5;
background-color:#517a9b;
}
QTableView QHeaderView:checked {
color:white;
font:15px;
background-color:#6D6D6D;
}

